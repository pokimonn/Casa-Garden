        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Casa_Garden"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Sign In" HorizontalAlignment="Left" Height="37" Margin="60,202,0,0" VerticalAlignment="Top" Width="128" Grid.Column="1" Click="Button_Click"/>
        <TextBlock x:Name="Titleblock" HorizontalAlignment="Left" Height="41" Margin="119,10,0,0" TextWrapping="Wrap" Text="Casa Gardens" VerticalAlignment="Top" Width="270" Grid.Column="1"/>
        <Menu x:Name="Logbutton" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="37" Margin="284,202,0,0" VerticalAlignment="Top" Width="118" AutomationProperties.Name="Log Vegitables"/>

    </Grid>
